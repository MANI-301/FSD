import java.util.Scanner;
class  evenOrOdd
{
	static Scanner sc = new Scanner(System.in);
public static void main(String[] args) 
	{
		System.out.print("Enter Any Num:");
		int number = sc.nextInt();
		
		if (number%2==0)
		{
			System.out.println("Even");
		}
		else
			System.out.println("odd");
		
	}
}
